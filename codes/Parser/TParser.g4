            (CAPACITOR_NAME WHITESPACE+ TEXT WHITESPACE+ TEXT WHITESPACE+ NUMERIC_VALUE* WHITESPACE* NEWLINE) |
            (CAPACITOR_NAME WHITESPACE+ TEXT WHITESPACE+ TEXT WHITESPACE* NEWLINE);

inductor :  (INDUCTOR_NAME WHITESPACE+ TEXT WHITESPACE+ TEXT WHITESPACE+ NUMERIC_VALUE+ WHITESPACE+ PARAMS* WHITESPACE* NEWLINE) |
            (INDUCTOR_NAME WHITESPACE+ TEXT WHITESPACE+ TEXT WHITESPACE+ NUMERIC_VALUE* WHITESPACE* NEWLINE) |
            (INDUCTOR_NAME WHITESPACE+ TEXT WHITESPACE+ TEXT WHITESPACE* NEWLINE);