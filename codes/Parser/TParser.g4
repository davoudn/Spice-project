parser grammar TParser;
options { tokenVocab=TLexer; }

main :  (resistor* capacitor* inductor*);
        // (main* capacitor* main*);// |
        // (main* inductor* main*); 

            
//4
resistor : (RESISTOR_NAME WHITESPACE+ TEXT WHITESPACE+ TEXT WHITESPACE+ NUMERIC_VALUE+ WHITESPACE+ PARAMS* WHITESPACE* NEWLINE) |
           (RESISTOR_NAME WHITESPACE+ TEXT WHITESPACE+ TEXT WHITESPACE+ NUMERIC_VALUE* WHITESPACE* NEWLINE) |
           (RESISTOR_NAME WHITESPACE+ TEXT WHITESPACE+ TEXT WHITESPACE* NEWLINE);

